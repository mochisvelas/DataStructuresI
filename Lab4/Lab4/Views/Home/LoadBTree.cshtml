
@{
    ViewBag.Title = "LoadBTree";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<center><h5 style="padding: 25px">Cargar fármacos al árbol B</h5></center>
<div class="formsgg">
        <form class="form" id="employees" action=@Url.Action("RegisterEmployee") method="post">
            <input type="number" class="textbox" name="order" min="0" max="2147483647" placeholder="Grado del árbol" required />
            <input type="submit" class="button-primary btn" name="Enter" value="Cargar" />
        </form>
</div>

